<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" styleClass="root" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.InitModelController">
    <children>
        <Label layoutX="14.0" layoutY="49.0" text="Name:" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <TextField fx:id="txtNameModel" layoutX="58.0" layoutY="45.0" AnchorPane.leftAnchor="70.0"
                   AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <Label layoutX="13.0" layoutY="85.0" text="Description:" AnchorPane.leftAnchor="10.0"
               AnchorPane.topAnchor="50.0"/>
        <TextArea fx:id="txaDescriptionModel" layoutX="58.0" layoutY="71.0" prefHeight="134.0" prefWidth="372.0"
                  wrapText="true" AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="10.0"
                  AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="80.0"/>
        <Button fx:id="btnSave" layoutX="334.0" layoutY="261.0" mnemonicParsing="false" onMouseClicked="#setModel"
                prefWidth="80.0" text="OK" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"/>
        <Button layoutX="19.0" layoutY="261.0" mnemonicParsing="false" onMouseClicked="#cancel" prefWidth="80.0"
                text="Cancel" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"/>
    </children>
</AnchorPane>
